{
    "presets": [
        ["env", {
            "targets": {
                "browsers": ["last 5 chrome versions"]
            }
        }]
    ],
    "plugins": [
        ["@babel/plugin-proposal-object-rest-spread"],
        ["@babel/plugin-proposal-class-properties",
            {
                "loose": true
            }
        ],
        ["@babel/syntax-dynamic-import"]
    ]
}
